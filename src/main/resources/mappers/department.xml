<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iyy.mapper.DepartmentMapper">
    <select id="selectByIdDepartment" resultType="com.iyy.entity.Department">
        SELECT * FROM tb_dept WHERE dept_id = #{id}
    </select>
    <select id="getDepartmentList" resultType="com.iyy.entity.Department">
        SELECT * FROM tb_dept
    </select>
</mapper>